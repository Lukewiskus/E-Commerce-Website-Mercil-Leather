    <li>\r\n                        {navBarDataSignedOut.map((item, index) => {\r\n                            return (\r\n                                <li key={index} className={item.className}>\r\n                                <Link to={item.path}>\r\n                                    {item.icon}\r\n                                    <span>{item.title}</span>\r\n                                </Link>\r\n                                </li>\r\n                            )\r\n                        })}\r\n                    </li>\r\n                </ul>]}\r\n                {currentUser && isAdmin && [\r\n                    <ul>\r\n                    <li>\r\n                        {navBarDataAuth.map((item, index) => {\r\n                            return (\r\n                                <li key={index} className={item.className}>\r\n                                <Link to={item.path}>\r\n                                    {item.icon}\r\n                                    <span>{item.title}</span>\r\n                                </Link>\r\n                                </li>\r\n                            )\r\n                        })}\r\n                    </li>\r\n                    <li>\r\n                        <span className=\"navText\" onClick={() => signOut()}>\r\n                            SignOut\r\n                        </span>\r\n                    </li>\r\n                </ul>\r\n                ]}\r\n                {currentUser && !isAdmin && [\r\n                    <ul>\r\n                    <li>\r\n                        {navBarDataSignedIn.map((item, index) => {\r\n                            return (\r\n                                <li key={index} className={item.className}>\r\n                                <Link to={item.path}>\r\n                                    {item.icon}\r\n                                    <span>{item.title}</span>\r\n                                </Link>\r\n            